{
  "expo": {
    "name": "medplus",
    "slug": "medplus",
    "version": "1.0.0",
    "scheme": "medplus",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon03.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "buildNumber": "medplus.company.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon01.png"
      },
      "package": "medplus.company.app"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "e9bb7c94-7804-4f46-930f-2243a5c33175"
      }
    }
  }
}
